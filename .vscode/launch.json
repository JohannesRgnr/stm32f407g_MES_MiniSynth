{
    "configurations": [
        {
            "showDevDebugOutput": "parsed",
            "cwd": "${workspaceRoot}",
            "executable": "./build/stm32f407g_disc_MidiHost_Sound.elf",
            "name": "Debug STM32",
            "request": "launch",
            "type": "cortex-debug",
            "liveWatch": {
                "enabled": true,
                "samplesPerSecond": 4
            },
            "servertype": "openocd",
            "preLaunchTask": "Build STM",
            "device": "stm32f407",
            "configFiles": [
                "openocd.cfg"
            ],
            "rttConfig": {
                "enabled": true,
                "address": "auto",
                "clearSearch": false,    // OpenOCD users may have to un-comment this
                "decoders": [
                    {
                        "port": 0,
                        "type": "console"
                    }
                ]
            },
            "svdFile": "STM32F407.svd"
        },
        {
            "showDevDebugOutput": "parsed",
            "cwd": "${workspaceRoot}",
            "executable": "./build/stm32f407g_disc_MidiHost_Sound.elf",
            "name": "Attach STM32",
            "request": "attach",
            "type": "cortex-debug",
            "servertype": "openocd",
            "preLaunchTask": "Build STM",
            "device": "stm32f407",
            "configFiles": [
                "openocd.cfg"
            ]
        }
    ]
}