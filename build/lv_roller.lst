 160:Drivers/lvgl/src/widgets/lv_roller.c ****     lv_roller_t * roller = (lv_roller_t *)obj;
 161:Drivers/lvgl/src/widgets/lv_roller.c **** 
 162:Drivers/lvgl/src/widgets/lv_roller.c ****     /*In infinite mode interpret the new ID relative to the currently visible "page"*/
 163:Drivers/lvgl/src/widgets/lv_roller.c ****     if(roller->mode == LV_ROLLER_MODE_INFINITE) {
 164:Drivers/lvgl/src/widgets/lv_roller.c ****         uint32_t real_option_cnt = roller->option_cnt / LV_ROLLER_INF_PAGES;
 165:Drivers/lvgl/src/widgets/lv_roller.c ****         uint16_t current_page = roller->sel_opt_id / real_option_cnt;
 166:Drivers/lvgl/src/widgets/lv_roller.c ****         /*Set by the user to e.g. 0, 1, 2, 3...
 167:Drivers/lvgl/src/widgets/lv_roller.c ****          *Upscale the value to the current page*/
 168:Drivers/lvgl/src/widgets/lv_roller.c ****         if(sel_opt < real_option_cnt) {
 169:Drivers/lvgl/src/widgets/lv_roller.c ****             uint16_t act_opt = roller->sel_opt_id - current_page * real_option_cnt;
 170:Drivers/lvgl/src/widgets/lv_roller.c ****             int32_t sel_opt_signed = sel_opt;
 171:Drivers/lvgl/src/widgets/lv_roller.c ****             /*Huge jump? Probably from last to first or first to last option.*/
 172:Drivers/lvgl/src/widgets/lv_roller.c ****             if(LV_ABS((int16_t)act_opt - sel_opt) > real_option_cnt / 2) {
 173:Drivers/lvgl/src/widgets/lv_roller.c ****                 if(act_opt > sel_opt) sel_opt_signed += real_option_cnt;
 174:Drivers/lvgl/src/widgets/lv_roller.c ****                 else sel_opt_signed -= real_option_cnt;
 175:Drivers/lvgl/src/widgets/lv_roller.c ****             }
 176:Drivers/lvgl/src/widgets/lv_roller.c ****             sel_opt = sel_opt_signed + real_option_cnt * current_page;
 177:Drivers/lvgl/src/widgets/lv_roller.c ****         }
 178:Drivers/lvgl/src/widgets/lv_roller.c ****     }
 179:Drivers/lvgl/src/widgets/lv_roller.c **** 
