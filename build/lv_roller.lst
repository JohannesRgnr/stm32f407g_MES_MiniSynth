 174:Drivers/lvgl/src/widgets/lv_roller.c ****                 else sel_opt_signed -= real_option_cnt;
 175:Drivers/lvgl/src/widgets/lv_roller.c ****             }
 176:Drivers/lvgl/src/widgets/lv_roller.c ****             sel_opt = sel_opt_signed + real_option_cnt * current_page;
 177:Drivers/lvgl/src/widgets/lv_roller.c ****         }
 178:Drivers/lvgl/src/widgets/lv_roller.c ****     }
 179:Drivers/lvgl/src/widgets/lv_roller.c **** 
