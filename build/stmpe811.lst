 159:Drivers/BSP/Components/stmpe811/stmpe811.c **** /**
 160:Drivers/BSP/Components/stmpe811/stmpe811.c ****   * @brief  Reset the stmpe811 by Software.
 161:Drivers/BSP/Components/stmpe811/stmpe811.c ****   * @param  DeviceAddr: Device address on communication Bus.  
 162:Drivers/BSP/Components/stmpe811/stmpe811.c ****   * @retval None
 163:Drivers/BSP/Components/stmpe811/stmpe811.c ****   */
 164:Drivers/BSP/Components/stmpe811/stmpe811.c **** void stmpe811_Reset(uint16_t DeviceAddr)
 165:Drivers/BSP/Components/stmpe811/stmpe811.c **** {
 166:Drivers/BSP/Components/stmpe811/stmpe811.c ****   /* Power Down the stmpe811 */  
 167:Drivers/BSP/Components/stmpe811/stmpe811.c ****   IOE_Write(DeviceAddr, STMPE811_REG_SYS_CTRL1, 2);
 168:Drivers/BSP/Components/stmpe811/stmpe811.c **** 
 169:Drivers/BSP/Components/stmpe811/stmpe811.c ****   /* Wait for a delay to ensure registers erasing */
 170:Drivers/BSP/Components/stmpe811/stmpe811.c ****   IOE_Delay(10); 
 171:Drivers/BSP/Components/stmpe811/stmpe811.c ****   
 172:Drivers/BSP/Components/stmpe811/stmpe811.c ****   /* Power On the Codec after the power off => all registers are reinitialized */
 173:Drivers/BSP/Components/stmpe811/stmpe811.c ****   IOE_Write(DeviceAddr, STMPE811_REG_SYS_CTRL1, 0);
 174:Drivers/BSP/Components/stmpe811/stmpe811.c ****   
 175:Drivers/BSP/Components/stmpe811/stmpe811.c ****   /* Wait for a delay to ensure registers erasing */
 176:Drivers/BSP/Components/stmpe811/stmpe811.c ****   IOE_Delay(2); 
 177:Drivers/BSP/Components/stmpe811/stmpe811.c **** }
 178:Drivers/BSP/Components/stmpe811/stmpe811.c **** 
 179:Drivers/BSP/Components/stmpe811/stmpe811.c **** /**
 180:Drivers/BSP/Components/stmpe811/stmpe811.c ****   * @brief  Read the stmpe811 IO Expander device ID.
 181:Drivers/BSP/Components/stmpe811/stmpe811.c ****   * @param  DeviceAddr: Device address on communication Bus.  
 182:Drivers/BSP/Components/stmpe811/stmpe811.c ****   * @retval The Device ID (two bytes).
 183:Drivers/BSP/Components/stmpe811/stmpe811.c ****   */
 184:Drivers/BSP/Components/stmpe811/stmpe811.c **** uint16_t stmpe811_ReadID(uint16_t DeviceAddr)
 185:Drivers/BSP/Components/stmpe811/stmpe811.c **** {
 186:Drivers/BSP/Components/stmpe811/stmpe811.c ****   /* Initialize IO BUS layer */
 187:Drivers/BSP/Components/stmpe811/stmpe811.c ****   IOE_Init(); 
 188:Drivers/BSP/Components/stmpe811/stmpe811.c ****   
 189:Drivers/BSP/Components/stmpe811/stmpe811.c ****   /* Return the device ID value */
 190:Drivers/BSP/Components/stmpe811/stmpe811.c ****   return ((IOE_Read(DeviceAddr, STMPE811_REG_CHP_ID_LSB) << 8) |\
 191:Drivers/BSP/Components/stmpe811/stmpe811.c ****           (IOE_Read(DeviceAddr, STMPE811_REG_CHP_ID_MSB)));
 192:Drivers/BSP/Components/stmpe811/stmpe811.c **** }
 193:Drivers/BSP/Components/stmpe811/stmpe811.c **** 
 194:Drivers/BSP/Components/stmpe811/stmpe811.c **** /**
 195:Drivers/BSP/Components/stmpe811/stmpe811.c ****   * @brief  Enable the Global interrupt.
 196:Drivers/BSP/Components/stmpe811/stmpe811.c ****   * @param  DeviceAddr: Device address on communication Bus.       
 197:Drivers/BSP/Components/stmpe811/stmpe811.c ****   * @retval None
 198:Drivers/BSP/Components/stmpe811/stmpe811.c ****   */
 199:Drivers/BSP/Components/stmpe811/stmpe811.c **** void stmpe811_EnableGlobalIT(uint16_t DeviceAddr)
 200:Drivers/BSP/Components/stmpe811/stmpe811.c **** {
 201:Drivers/BSP/Components/stmpe811/stmpe811.c ****   uint8_t tmp = 0;
ARM GAS  /var/folders/0z/tsy30v1j4xl4_7kw94k978ww0000gn/T//ccoIAUDL.s 			page 5


 202:Drivers/BSP/Components/stmpe811/stmpe811.c ****   
 203:Drivers/BSP/Components/stmpe811/stmpe811.c ****   /* Read the Interrupt Control register  */
 204:Drivers/BSP/Components/stmpe811/stmpe811.c ****   tmp = IOE_Read(DeviceAddr, STMPE811_REG_INT_CTRL);
 205:Drivers/BSP/Components/stmpe811/stmpe811.c ****   
 206:Drivers/BSP/Components/stmpe811/stmpe811.c ****   /* Set the global interrupts to be Enabled */    
 207:Drivers/BSP/Components/stmpe811/stmpe811.c ****   tmp |= (uint8_t)STMPE811_GIT_EN;
 208:Drivers/BSP/Components/stmpe811/stmpe811.c ****   
 209:Drivers/BSP/Components/stmpe811/stmpe811.c ****   /* Write Back the Interrupt Control register */
 210:Drivers/BSP/Components/stmpe811/stmpe811.c ****   IOE_Write(DeviceAddr, STMPE811_REG_INT_CTRL, tmp); 
 211:Drivers/BSP/Components/stmpe811/stmpe811.c **** }
