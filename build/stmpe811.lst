 126:Drivers/BSP/Components/stmpe811/stmpe811.c **** /**
 127:Drivers/BSP/Components/stmpe811/stmpe811.c ****   * @brief  Initialize the stmpe811 and configure the needed hardware resources
 128:Drivers/BSP/Components/stmpe811/stmpe811.c ****   * @param  DeviceAddr: Device address on communication Bus.
 129:Drivers/BSP/Components/stmpe811/stmpe811.c ****   * @retval None
 130:Drivers/BSP/Components/stmpe811/stmpe811.c ****   */
 131:Drivers/BSP/Components/stmpe811/stmpe811.c **** void stmpe811_Init(uint16_t DeviceAddr)
 132:Drivers/BSP/Components/stmpe811/stmpe811.c **** {
 133:Drivers/BSP/Components/stmpe811/stmpe811.c ****   uint8_t instance;
 134:Drivers/BSP/Components/stmpe811/stmpe811.c ****   uint8_t empty;
 135:Drivers/BSP/Components/stmpe811/stmpe811.c ****   
 136:Drivers/BSP/Components/stmpe811/stmpe811.c ****   /* Check if device instance already exists */
 137:Drivers/BSP/Components/stmpe811/stmpe811.c ****   instance = stmpe811_GetInstance(DeviceAddr);
 138:Drivers/BSP/Components/stmpe811/stmpe811.c ****   
 139:Drivers/BSP/Components/stmpe811/stmpe811.c ****   /* To prevent double initialization */
 140:Drivers/BSP/Components/stmpe811/stmpe811.c ****   if(instance == 0xFF)
 141:Drivers/BSP/Components/stmpe811/stmpe811.c ****   {
 142:Drivers/BSP/Components/stmpe811/stmpe811.c ****     /* Look for empty instance */
 143:Drivers/BSP/Components/stmpe811/stmpe811.c ****     empty = stmpe811_GetInstance(0);
 144:Drivers/BSP/Components/stmpe811/stmpe811.c ****     
ARM GAS  /var/folders/0z/tsy30v1j4xl4_7kw94k978ww0000gn/T//ccoIAUDL.s 			page 4


 145:Drivers/BSP/Components/stmpe811/stmpe811.c ****     if(empty < STMPE811_MAX_INSTANCE)
 146:Drivers/BSP/Components/stmpe811/stmpe811.c ****     {
 147:Drivers/BSP/Components/stmpe811/stmpe811.c ****       /* Register the current device instance */
 148:Drivers/BSP/Components/stmpe811/stmpe811.c ****       stmpe811[empty] = DeviceAddr;
 149:Drivers/BSP/Components/stmpe811/stmpe811.c ****       
 150:Drivers/BSP/Components/stmpe811/stmpe811.c ****       /* Initialize IO BUS layer */
 151:Drivers/BSP/Components/stmpe811/stmpe811.c ****       IOE_Init(); 
 152:Drivers/BSP/Components/stmpe811/stmpe811.c ****       
 153:Drivers/BSP/Components/stmpe811/stmpe811.c ****       /* Generate stmpe811 Software reset */
 154:Drivers/BSP/Components/stmpe811/stmpe811.c ****       stmpe811_Reset(DeviceAddr);
 155:Drivers/BSP/Components/stmpe811/stmpe811.c ****     }
 156:Drivers/BSP/Components/stmpe811/stmpe811.c ****   }
 157:Drivers/BSP/Components/stmpe811/stmpe811.c **** }
