 167:Drivers/BSP/Components/stmpe1600/stmpe1600.c ****   
 168:Drivers/BSP/Components/stmpe1600/stmpe1600.c ****   /* Power On the Codec after the power off: all registers are reinitialized */
 169:Drivers/BSP/Components/stmpe1600/stmpe1600.c ****   IOE_Write(DeviceAddr, STMPE1600_REG_SYS_CTRL, (uint16_t)0x00); 
 170:Drivers/BSP/Components/stmpe1600/stmpe1600.c ****   
 171:Drivers/BSP/Components/stmpe1600/stmpe1600.c ****   /* Wait for a delay to ensure registers erasing */
 172:Drivers/BSP/Components/stmpe1600/stmpe1600.c ****   IOE_Delay(2); 
 173:Drivers/BSP/Components/stmpe1600/stmpe1600.c **** }
