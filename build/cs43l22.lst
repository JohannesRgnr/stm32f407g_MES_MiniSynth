 125:Drivers/BSP/Components/cs43l22/cs43l22.c **** /**
 126:Drivers/BSP/Components/cs43l22/cs43l22.c ****   * @brief Initializes the audio codec and the control interface.
 127:Drivers/BSP/Components/cs43l22/cs43l22.c ****   * @param DeviceAddr: Device address on communication Bus.   
 128:Drivers/BSP/Components/cs43l22/cs43l22.c ****   * @param OutputDevice: can be OUTPUT_DEVICE_SPEAKER, OUTPUT_DEVICE_HEADPHONE,
 129:Drivers/BSP/Components/cs43l22/cs43l22.c ****   *                       OUTPUT_DEVICE_BOTH or OUTPUT_DEVICE_AUTO .
 130:Drivers/BSP/Components/cs43l22/cs43l22.c ****   * @param Volume: Initial volume level (from 0 (Mute) to 100 (Max))
 131:Drivers/BSP/Components/cs43l22/cs43l22.c ****   * @retval 0 if correct communication, else wrong communication
 132:Drivers/BSP/Components/cs43l22/cs43l22.c ****   */
 133:Drivers/BSP/Components/cs43l22/cs43l22.c **** uint32_t cs43l22_Init(uint16_t DeviceAddr, uint16_t OutputDevice, uint8_t Volume, uint32_t AudioFre
 134:Drivers/BSP/Components/cs43l22/cs43l22.c **** {
 135:Drivers/BSP/Components/cs43l22/cs43l22.c ****   uint32_t counter = 0;
 136:Drivers/BSP/Components/cs43l22/cs43l22.c ****   
 137:Drivers/BSP/Components/cs43l22/cs43l22.c ****   /* Initialize the Control interface of the Audio Codec */
 138:Drivers/BSP/Components/cs43l22/cs43l22.c ****   AUDIO_IO_Init();     
 139:Drivers/BSP/Components/cs43l22/cs43l22.c ****     
 140:Drivers/BSP/Components/cs43l22/cs43l22.c ****   /* Keep Codec powered OFF */
 141:Drivers/BSP/Components/cs43l22/cs43l22.c ****   counter += CODEC_IO_Write(DeviceAddr, CS43L22_REG_POWER_CTL1, 0x01);  
 142:Drivers/BSP/Components/cs43l22/cs43l22.c ****   
ARM GAS  /var/folders/lp/x54gwt890xv47sdkqj1k04q40000gn/T//ccQqrKkN.s 			page 4


 143:Drivers/BSP/Components/cs43l22/cs43l22.c ****   /*Save Output device for mute ON/OFF procedure*/
 144:Drivers/BSP/Components/cs43l22/cs43l22.c ****   switch (OutputDevice)
 145:Drivers/BSP/Components/cs43l22/cs43l22.c ****   {
 146:Drivers/BSP/Components/cs43l22/cs43l22.c ****   case OUTPUT_DEVICE_SPEAKER:
 147:Drivers/BSP/Components/cs43l22/cs43l22.c ****     OutputDev = 0xFA;
 148:Drivers/BSP/Components/cs43l22/cs43l22.c ****     break;
 149:Drivers/BSP/Components/cs43l22/cs43l22.c ****     
 150:Drivers/BSP/Components/cs43l22/cs43l22.c ****   case OUTPUT_DEVICE_HEADPHONE:
 151:Drivers/BSP/Components/cs43l22/cs43l22.c ****     OutputDev = 0xAF;
 152:Drivers/BSP/Components/cs43l22/cs43l22.c ****     break;
 153:Drivers/BSP/Components/cs43l22/cs43l22.c ****     
 154:Drivers/BSP/Components/cs43l22/cs43l22.c ****   case OUTPUT_DEVICE_BOTH:
 155:Drivers/BSP/Components/cs43l22/cs43l22.c ****     OutputDev = 0xAA;
 156:Drivers/BSP/Components/cs43l22/cs43l22.c ****     break;
 157:Drivers/BSP/Components/cs43l22/cs43l22.c ****     
 158:Drivers/BSP/Components/cs43l22/cs43l22.c ****   case OUTPUT_DEVICE_AUTO:
 159:Drivers/BSP/Components/cs43l22/cs43l22.c ****     OutputDev = 0x05;
 160:Drivers/BSP/Components/cs43l22/cs43l22.c ****     break;    
 161:Drivers/BSP/Components/cs43l22/cs43l22.c ****     
 162:Drivers/BSP/Components/cs43l22/cs43l22.c ****   default:
 163:Drivers/BSP/Components/cs43l22/cs43l22.c ****     OutputDev = 0x05;
 164:Drivers/BSP/Components/cs43l22/cs43l22.c ****     break;    
 165:Drivers/BSP/Components/cs43l22/cs43l22.c ****   }
 166:Drivers/BSP/Components/cs43l22/cs43l22.c ****   
 167:Drivers/BSP/Components/cs43l22/cs43l22.c ****   counter += CODEC_IO_Write(DeviceAddr, CS43L22_REG_POWER_CTL2, OutputDev);
 168:Drivers/BSP/Components/cs43l22/cs43l22.c ****   
 169:Drivers/BSP/Components/cs43l22/cs43l22.c ****   /* Clock configuration: Auto detection */  
 170:Drivers/BSP/Components/cs43l22/cs43l22.c ****   counter += CODEC_IO_Write(DeviceAddr, CS43L22_REG_CLOCKING_CTL, 0x81);
 171:Drivers/BSP/Components/cs43l22/cs43l22.c ****   
 172:Drivers/BSP/Components/cs43l22/cs43l22.c ****   /* Set the Slave Mode and the audio Standard */  
 173:Drivers/BSP/Components/cs43l22/cs43l22.c ****   counter += CODEC_IO_Write(DeviceAddr, CS43L22_REG_INTERFACE_CTL1, CODEC_STANDARD);
 174:Drivers/BSP/Components/cs43l22/cs43l22.c ****   
 175:Drivers/BSP/Components/cs43l22/cs43l22.c ****   /* Set the Master volume */
 176:Drivers/BSP/Components/cs43l22/cs43l22.c ****   counter += cs43l22_SetVolume(DeviceAddr, Volume);
 177:Drivers/BSP/Components/cs43l22/cs43l22.c ****   
 178:Drivers/BSP/Components/cs43l22/cs43l22.c ****   /* If the Speaker is enabled, set the Mono mode and volume attenuation level */
 179:Drivers/BSP/Components/cs43l22/cs43l22.c ****   if(OutputDevice != OUTPUT_DEVICE_HEADPHONE)
 180:Drivers/BSP/Components/cs43l22/cs43l22.c ****   {
 181:Drivers/BSP/Components/cs43l22/cs43l22.c ****     /* Set the Speaker Mono mode */  
 182:Drivers/BSP/Components/cs43l22/cs43l22.c ****     counter += CODEC_IO_Write(DeviceAddr, CS43L22_REG_PLAYBACK_CTL2, 0x06);
 183:Drivers/BSP/Components/cs43l22/cs43l22.c ****     
 184:Drivers/BSP/Components/cs43l22/cs43l22.c ****     /* Set the Speaker attenuation level */  
 185:Drivers/BSP/Components/cs43l22/cs43l22.c ****     counter += CODEC_IO_Write(DeviceAddr, CS43L22_REG_SPEAKER_A_VOL, 0x00);
 186:Drivers/BSP/Components/cs43l22/cs43l22.c ****     counter += CODEC_IO_Write(DeviceAddr, CS43L22_REG_SPEAKER_B_VOL, 0x00);
 187:Drivers/BSP/Components/cs43l22/cs43l22.c ****   }
 188:Drivers/BSP/Components/cs43l22/cs43l22.c ****   
 189:Drivers/BSP/Components/cs43l22/cs43l22.c ****   /* Additional configuration for the CODEC. These configurations are done to reduce
 190:Drivers/BSP/Components/cs43l22/cs43l22.c ****   the time needed for the Codec to power off. If these configurations are removed, 
 191:Drivers/BSP/Components/cs43l22/cs43l22.c ****   then a long delay should be added between powering off the Codec and switching 
 192:Drivers/BSP/Components/cs43l22/cs43l22.c ****   off the I2S peripheral MCLK clock (which is the operating clock for Codec).
 193:Drivers/BSP/Components/cs43l22/cs43l22.c ****   If this delay is not inserted, then the codec will not shut down properly and
 194:Drivers/BSP/Components/cs43l22/cs43l22.c ****   it results in high noise after shut down. */
 195:Drivers/BSP/Components/cs43l22/cs43l22.c ****   
 196:Drivers/BSP/Components/cs43l22/cs43l22.c ****   /* Disable the analog soft ramp */
 197:Drivers/BSP/Components/cs43l22/cs43l22.c ****   counter += CODEC_IO_Write(DeviceAddr, CS43L22_REG_ANALOG_ZC_SR_SETT, 0x00);
 198:Drivers/BSP/Components/cs43l22/cs43l22.c ****   /* Disable the digital soft ramp */
 199:Drivers/BSP/Components/cs43l22/cs43l22.c ****   counter += CODEC_IO_Write(DeviceAddr, CS43L22_REG_MISC_CTL, 0x04);
ARM GAS  /var/folders/lp/x54gwt890xv47sdkqj1k04q40000gn/T//ccQqrKkN.s 			page 5


 200:Drivers/BSP/Components/cs43l22/cs43l22.c ****   /* Disable the limiter attack level */
 201:Drivers/BSP/Components/cs43l22/cs43l22.c ****   counter += CODEC_IO_Write(DeviceAddr, CS43L22_REG_LIMIT_CTL1, 0x00);
 202:Drivers/BSP/Components/cs43l22/cs43l22.c ****   /* Adjust Bass and Treble levels */
 203:Drivers/BSP/Components/cs43l22/cs43l22.c ****   counter += CODEC_IO_Write(DeviceAddr, CS43L22_REG_TONE_CTL, 0x0F);
 204:Drivers/BSP/Components/cs43l22/cs43l22.c ****   /* Adjust PCM volume level */
 205:Drivers/BSP/Components/cs43l22/cs43l22.c ****   counter += CODEC_IO_Write(DeviceAddr, CS43L22_REG_PCMA_VOL, 0x0A);
 206:Drivers/BSP/Components/cs43l22/cs43l22.c ****   counter += CODEC_IO_Write(DeviceAddr, CS43L22_REG_PCMB_VOL, 0x0A);
 207:Drivers/BSP/Components/cs43l22/cs43l22.c ****   
 208:Drivers/BSP/Components/cs43l22/cs43l22.c ****   /* Return communication control value */
 209:Drivers/BSP/Components/cs43l22/cs43l22.c ****   return counter;  
 210:Drivers/BSP/Components/cs43l22/cs43l22.c **** }
