 301:Drivers/BSP/Components/cs43l22/cs43l22.c ****   counter += CODEC_IO_Write(DeviceAddr, CS43L22_REG_POWER_CTL2, OutputDev);
 302:Drivers/BSP/Components/cs43l22/cs43l22.c **** 
 303:Drivers/BSP/Components/cs43l22/cs43l22.c ****   /* Exit the Power save mode */
 304:Drivers/BSP/Components/cs43l22/cs43l22.c ****   counter += CODEC_IO_Write(DeviceAddr, CS43L22_REG_POWER_CTL1, 0x9E); 
 305:Drivers/BSP/Components/cs43l22/cs43l22.c ****   
 306:Drivers/BSP/Components/cs43l22/cs43l22.c ****   return counter;
 307:Drivers/BSP/Components/cs43l22/cs43l22.c **** }
